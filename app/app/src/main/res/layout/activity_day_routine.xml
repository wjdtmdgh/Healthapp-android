<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DayRoutineActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textColor="#50BCDF"
        android:text="정승호님의 하루 운동 추천 ROUTINE"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회원님은 현재 가슴 쪽 운동이 많이 부족한 상태입니다."
        android:textStyle="bold"
        android:textSize="16sp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <ImageView
        android:id="@+id/body"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="80dp"
        android:src="@drawable/body"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <ImageView
        android:id="@+id/img_right_up"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/e9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/body" />

    <ImageView
        android:id="@+id/arrow_right_up"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_right_up"
        app:layout_constraintEnd_toStartOf="@+id/img_right_up"
        app:layout_constraintTop_toTopOf="@+id/img_right_up" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="1SET:10 \n2SET:7 \n3SET:4"
        app:layout_constraintEnd_toEndOf="@+id/img_right_up"
        app:layout_constraintStart_toStartOf="@+id/arrow_right_up"
        app:layout_constraintTop_toBottomOf="@+id/img_right_up" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="1SET:20X10 \n2SET:20X7 \n3SET:20X4"
        app:layout_constraintEnd_toEndOf="@+id/img_right_down"
        app:layout_constraintStart_toStartOf="@+id/arrow_right_down"
        app:layout_constraintTop_toBottomOf="@+id/img_right_down" />


    <TextView
        android:id="@+id/tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1SET:40X10 \n2SET:40X7 \n3SET:40X4"
        android:textStyle="bold"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/arrow_left_up"
        app:layout_constraintStart_toStartOf="@+id/img_left_up"
        app:layout_constraintTop_toBottomOf="@+id/img_left_up" />


    <TextView
        android:id="@+id/tv4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1SET:10X10 \n2SET:10X7 \n3SET:10X4"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/arrow_left_down"
        app:layout_constraintStart_toStartOf="@+id/img_left_down"
        app:layout_constraintTop_toBottomOf="@+id/img_left_down" />


    <ImageView
        android:id="@+id/img_right_down"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/e3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="170dp"/>

    <ImageView
        android:id="@+id/arrow_right_down"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_right_down"
        app:layout_constraintEnd_toStartOf="@+id/img_right_down"
        app:layout_constraintTop_toTopOf="@+id/img_right_down" />


    <ImageView
        android:id="@+id/img_left_down"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="160dp"
        android:src="@drawable/e1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />


    <ImageView
        android:id="@+id/arrow_left_down"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_left_down"
        app:layout_constraintStart_toEndOf="@+id/img_left_down"
        app:layout_constraintTop_toTopOf="@+id/img_left_down" />

    <ImageView
        android:id="@+id/img_left_up"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginBottom="150dp"
        android:src="@drawable/e11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/img_right_up" />


    <ImageView
        android:id="@+id/arrow_left_up"
        android:layout_width="60dp"
        android:layout_height="60dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_left_up"
        app:layout_constraintStart_toEndOf="@+id/img_left_up"
        app:layout_constraintTop_toTopOf="@+id/img_left_up" />

    <Button
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        android:background="#bcbcbc"
        android:text="prev"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>